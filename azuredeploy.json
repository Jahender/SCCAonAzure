{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string",
      "defaultValue": "VDSS_VNet",
      "metadata": {
        "description": "Name for the new VNet."
      }
    },
    "vnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.0.0/16",
      "metadata": {
        "description": "CIDR prefix for the VNet address space."
      }
    },
    "vnetPrefix2": {
      "type": "string",
      "defaultValue": "172.16.0.0/23",
      "metadata": {
        "description": "CIDR prefix for the Second VNet address space."
      }
    },
    "F5_Ext_Untrusted_SubnetName": {
      "type": "string",
      "defaultValue": "F5_Ext_Untrusted_Subnet",
      "metadata": {
        "description": "External F5 Control Plane Subnet."
      }
    },
    "F5_Ext_Untrusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.0.0/24",
      "metadata": {
        "description": "CIDR address prefix for the External F5 Control Plane subnet."
      }
    },
    "F5_Ext_Trusted_SubnetName": {
      "type": "string",
      "defaultValue": "F5_Ext_Trusted_Subnet",
      "metadata": {
        "description": "Name for the F5_Ext_Trusted_Subnet subnet."
      }
    },
    "F5_Ext_Trusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.1.0/24",
      "metadata": {
        "description": "CIDR address prefix for the F5_External subnet."
      }
    },
    "IPS_Untrusted_SubnetName": {
      "type": "string",
      "defaultValue": "IPS_Untrusted_Subnet",
      "metadata": {
        "description": "Name for the IPS_Untrusted_Subnet subnet."
      }
    },
    "IPS_Untrusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.2.0/24",
      "metadata": {
        "description": "CIDR address prefix for the IPS_Untrusted_Subnet."
      }
    },
    "IPS_Trusted_SubnetName": {
      "type": "string",
      "defaultValue": "IPS_Trusted_Subnet",
      "metadata": {
        "description": "Name for the IPS_Trusted_Subnet subnet."
      }
    },
    "IPS_Trusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.3.0/24",
      "metadata": {
        "description": "CIDR address prefix for the IPS_Trusted_Subnet."
      }
    },
    "F5_Int_Untrusted_SubnetName": {
      "type": "string",
      "defaultValue": "F5_Int_Untrusted_Subnet",
      "metadata": {
        "description": "Name for the F5_Int_Untrusted subnet."
      }
    },
    "F5_Int_Untrusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.4.0/24",
      "metadata": {
        "description": "CIDR address prefix for the F5_Int_Untrusted subnet."
      }
    },
    "F5_Int_Trusted_SubnetName": {
      "type": "string",
      "defaultValue": "F5_Int_Trusted_Subnet",
      "metadata": {
        "description": "Name for the F5_Int_Trusted_Subnet."
      }
    },
    "F5_Int_Trusted_SubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.5.0/24",
      "metadata": {
        "description": "CIDR address prefix for the F5_Internal subnet."
      }
    },
    "Management_SubnetName": {
      "type": "string",
      "defaultValue": "Management_Subnet",
      "metadata": {
        "description": "Name for the Management_Subnet."
      }
    },
    "Management_SubnetPrefix": {
      "type": "string",
      "defaultValue": "172.16.0.0/24",
      "metadata": {
        "description": "CIDR address prefix for the Management_Subnet."
      }
    },
    "VDMS_SubnetName": {
      "type": "string",
      "defaultValue": "VDMS_Subnet",
      "metadata": {
        "description": "Name for the VDMS_Subnet."
      }
    },
    "VDMS_SubnetPrefix": {
      "type": "string",
      "defaultValue": "172.16.1.0/24",
      "metadata": {
        "description": "CIDR address prefix for the VDMS_Subnet."
      }
    },
    "GatewaySubnetName": {
      "type": "string",
      "defaultValue": "GatewaySubnet",
      "metadata": {
        "description": "Name for the GatewaySubnet (This must Stay Default Value)."
      }
    },
    "GatewaySubnetPrefix": {
      "type": "string",
      "defaultValue": "192.168.255.224/27",
      "metadata": {
        "description": "CIDR address prefix for the GatewaySubnet."
      }
    },
    "F5_Ext_Trusted_IP": {
      "type": "string",
      "defaultValue": "192.168.1.5",
      "metadata": {
        "description": "IP address for F5_Ext_Trusted_IP (For UDR Next Hop)."
      }
    },
    "F5_Int_Trusted_IP": {
      "type": "string",
      "defaultValue": "192.168.5.5",
      "metadata": {
        "description": "IP address for F5_Int_Trusted_IP (For UDR Next Hop)."
      }
    },
    "F5_Ext_Untrusted_IP": {
      "type": "string",
      "defaultValue": "192.168.0.7",
      "metadata": {
        "description": "IP address for F5_Ext_Untrusted_IP (For UDR Next Hop)."
      }
    },
    "F5_Int_Untrusted_IP": {
      "type": "string",
      "defaultValue": "192.168.4.7",
      "metadata": {
        "description": "IP address for F5_Int_Untrusted_IP (For UDR Next Hop)."
      }
    },
    "IPS_Untrusted_IP": {
      "type": "string",
      "defaultValue": "192.168.2.5",
      "metadata": {
        "description": "IP address for IPS_Untrusted_IP (For UDR Next Hop)."
      }
    },
    "IPS_Trusted_IP": {
      "type": "string",
      "defaultValue": "192.168.3.5",
      "metadata": {
        "description": "IP address for IPS_Trusted_IP (For UDR Next Hop)."
      }
    },
    "Gateway_RouteTableName": {
      "type": "string",
      "defaultValue": "Gateway_RouteTable",
      "metadata": {
        "description": "Name for Gateway_RouteTable."
      }
    },
    "F5_Ext_Untrust_RouteTableName": {
      "type": "string",
      "defaultValue": "F5_Ext_Untrust_RouteTable",
      "metadata": {
        "description": "Name for F5_Ext_Untrust_RouteTable."
      }
    },
    "IPS_Untrust_RouteTableName": {
      "type": "string",
      "defaultValue": "IPS_Untrust_RouteTable",
      "metadata": {
        "description": "Name for IPS_Untrust_RouteTable."
      }
    },
    "IPS_Trust_RouteTableName": {
      "type": "string",
      "defaultValue": "IPS_Trust_RouteTable",
      "metadata": {
        "description": "Name for IPS_Trust_RouteTable."
      }
    },
    "F5_Int_Untrust_RouteTableName": {
      "type": "string",
      "defaultValue": "F5_Int_Untrust_RouteTable",
      "metadata": {
        "description": "Name for F5_Int_Untrust_RouteTable."
      }
    },
    "F5_Ext_Trust_RouteTableName": {
      "type": "string",
      "defaultValue": "F5_Ext_Trust_RouteTable",
      "metadata": {
        "description": "Name for F5_Ext_Trust_RouteTable."
      }
    },
    "Internal_Subnets_RouteTableName": {
      "type": "string",
      "defaultValue": "Internal_Subnets_RouteTable",
      "metadata": {
        "description": "Name for Internal_Subnets_RouteTable."
      }
    },
    "VDSSJumpBox_Availability_SetName": {
      "type": "string",
      "defaultValue": "VDSSJumpBox_AS",
      "minLength": 1
    },
    "IPS_Availability_SetName": {
      "type": "string",
      "defaultValue": "IPS_AS",
      "minLength": 1
    },
    "vdssstorageType": {
      "type": "string",
      "defaultValue": "Standard_LRS",
      "allowedValues": [
        "Standard_LRS",
        "Standard_ZRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Premium_LRS"
      ]
    },
    "VDSSJumpBoxName": {
      "type": "string",
      "defaultValue": "VDSSJumpBox",
      "minLength": 1
    },
    "VDSSJumpBoxIp": {
      "type": "string",
      "defaultValue": "172.16.0.4",
      "metadata": {
        "description": "IP address for VDSSJumpBox."
      }
    },
    "VDSSJumpBoxAdminUserName": {
      "type": "string",
      "minLength": 1
    },
    "VDSSJumpBoxAdminPassword": {
      "type": "securestring"
    },
    "VDSSJumpBoxWindowsOSVersion": {
      "type": "string",
      "defaultValue": "2016-Datacenter",
      "allowedValues": [
        "2008-R2-SP1",
        "2012-Datacenter",
        "2012-R2-Datacenter",
        "2016-Datacenter",
        "Windows-Server-Technical-Preview"
      ]
    },
    "VDSSJumpBoxLinuxName": {
      "type": "string",
      "defaultValue": "VDSSJumpBoxLinux",
      "minLength": 1
    },
    "VDSSJumpBoxLinuxIp": {
      "type": "string",
      "defaultValue": "172.16.0.5",
      "metadata": {
        "description": "IP address for Linux VDSSJumpBox."
      }
    },
    "VDSSJumpBoxLinuxUbuntuOSVersion": {
      "type": "string",
      "defaultValue": "16.04-LTS",
      "allowedValues": [
        "12.04.5-LTS",
        "14.04.2-LTS",
        "16.04-LTS"
      ]
    },
    "IPSvmName": {
      "type": "string",
      "defaultValue": "IPSvm",
      "minLength": 1
    },
    "IPS_Untrusted_NIC_Name": {
      "type": "string",
      "defaultValue": "ipsuntrustednic",
      "minLength": 1
    },
    "IPS_Trusted_NIC_Name": {
      "type": "string",
      "defaultValue": "ipstrustednic",
      "minLength": 1
    },
    "IPSvmUbuntuOSVersion": {
      "type": "string",
      "defaultValue": "16.04-LTS",
      "allowedValues": [
        "12.04.5-LTS",
        "14.04.2-LTS",
        "16.04-LTS"
      ]
    },
    "MOJumpBox_Availability_SetName": {
      "type": "string",
      "defaultValue": "MOJumpBox_AS",
      "minLength": 1
    },
    "MOJumpBoxName": {
      "type": "string",
      "defaultValue": "MOJumpBox",
      "minLength": 1
    },
    "MOJumpBoxIp": {
      "type": "string",
      "defaultValue": "172.16.0.6",
      "metadata": {
        "description": "IP address for MOJumpBox."
      }
    },
    "MOJumpBoxAdminUserName": {
      "type": "string",
      "minLength": 1
    },
    "MOJumpBoxAdminPassword": {
      "type": "securestring"
    },
    "MOJumpBoxWindowsOSVersion": {
      "type": "string",
      "defaultValue": "2016-Datacenter",
      "allowedValues": [
        "2008-R2-SP1",
        "2012-Datacenter",
        "2012-R2-Datacenter",
        "2016-Datacenter",
        "Windows-Server-Technical-Preview"
      ]
    },
    "MOJumpBoxLinuxName": {
      "type": "string",
      "defaultValue": "MOJumpBoxLinux",
      "minLength": 1
    },
    "MOJumpBoxLinuxIp": {
      "type": "string",
      "defaultValue": "172.16.0.7",
      "metadata": {
        "description": "IP address for Linux MOJumpBox."
      }
    },
    "MOJumpBoxLinuxUbuntuOSVersion": {
      "type": "string",
      "defaultValue": "16.04-LTS",
      "allowedValues": [
        "12.04.5-LTS",
        "14.04.2-LTS",
        "16.04-LTS"
      ]
    },
    "MOJumpBoxLinux_Availability_SetName": {
      "type": "string",
      "defaultValue": "MOJumpBoxLinux_AS",
      "minLength": 1
    },
    "VDSSJumpBoxLinux_Availability_SetName": {
      "type": "string",
      "defaultValue": "VDSSJumpBoxLinux_AS",
      "minLength": 1
    }
  },
  "variables": {
    "MOJumpBoxImagePublisher": "MicrosoftWindowsServer",
    "MOJumpBoxImageOffer": "WindowsServer",
    "MOJumpBoxOSDiskName": "MOJumpBoxOSDisk",
    "MOJumpBoxVmSize": "Standard_DS2_v2",
    "MOJumpBoxVnetID": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "MOJumpBoxSubnetRef": "[concat(variables('MOJumpBoxVnetID'), '/subnets/', parameters('Management_SubnetName'))]",
    "MOJumpBoxStorageAccountContainerName": "vhds",
    "MOJumpBoxNicName": "[concat(parameters('MOJumpBoxName'), 'NetworkInterface')]",
    "MOJumpBoxLinuxImagePublisher": "Canonical",
    "MOJumpBoxLinuxImageOffer": "UbuntuServer",
    "MOJumpBoxLinuxOSDiskName": "MOJumpBoxLinuxOSDisk",
    "MOJumpBoxLinuxVmSize": "Standard_DS2_v2",
    "MOJumpBoxLinuxVnetID": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "MOJumpBoxLinuxSubnetRef": "[concat(variables('MOJumpBoxLinuxVnetID'), '/subnets/', parameters('Management_SubnetName'))]",
    "MOJumpBoxLinuxStorageAccountContainerName": "vhds",
    "MOJumpBoxLinuxNicName": "[concat(parameters('MOJumpBoxLinuxName'), 'NetworkInterface')]",
    "linux-MOJumpBox-ipName": "linux-MOJumpBox-ip",
    "vdssstorageName": "[concat('vdssstorage', uniqueString(resourceGroup().id))]",
    "VDSSJumpBoxImagePublisher": "MicrosoftWindowsServer",
    "VDSSJumpBoxImageOffer": "WindowsServer",
    "VDSSJumpBoxOSDiskName": "VDSSJumpBoxOSDisk",
    "VDSSJumpBoxVmSize": "Standard_DS2_v2",
    "VDSSJumpBoxVnetID": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "VDSSJumpBoxSubnetRef": "[concat(variables('VDSSJumpBoxVnetID'), '/subnets/', parameters('Management_SubnetName'))]",
    "VDSSJumpBoxStorageAccountContainerName": "vhds",
    "VDSSJumpBoxNicName": "[concat(parameters('VDSSJumpBoxName'), 'NetworkInterface')]",
    "VDSSJumpBoxLinuxImagePublisher": "Canonical",
    "VDSSJumpBoxLinuxImageOffer": "UbuntuServer",
    "VDSSJumpBoxLinuxOSDiskName": "VDSSJumpBoxLinuxOSDisk",
    "VDSSJumpBoxLinuxVmSize": "Standard_DS2_v2",
    "VDSSJumpBoxLinuxVnetID": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "VDSSJumpBoxLinuxSubnetRef": "[concat(variables('VDSSJumpBoxLinuxVnetID'), '/subnets/', parameters('Management_SubnetName'))]",
    "VDSSJumpBoxLinuxStorageAccountContainerName": "vhds",
    "VDSSJumpBoxLinuxNicName": "[concat(parameters('VDSSJumpBoxLinuxName'), 'NetworkInterface')]",
    "linux-VDSSJumpBox-ipName": "linux-VDSSJumpBox-ip",
    "IPSvmImagePublisher": "Canonical",
    "IPSvmImageOffer": "UbuntuServer",
    "IPSvmOSDiskName": "IPSvmOSDisk",
    "IPSvmVmSize": "Standard_DS3_v2",
    "IPSvmVnetID": "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
    "IPSUntrustedSubnetRef": "[concat(variables('IPSvmVnetID'), '/subnets/', parameters('IPS_Untrusted_SubnetName'))]",
    "IPSTrustedSubnetRef": "[concat(variables('IPSvmVnetID'), '/subnets/', parameters('IPS_Trusted_SubnetName'))]",
    "IPSvmStorageAccountContainerName": "vhds",
    "IPSvmUntrustedNicName": "[concat(parameters('IPS_Untrusted_NIC_Name'), 'NetworkInterface')]",
    "IPSvmTrustedNicName": "[concat(parameters('IPS_Trusted_NIC_Name'), 'NetworkInterface')]",
    "IPSvmpipName": "IPSpip",
    "IPSCustomData": "#cloud-config\nwrite_files:\n\n\n##################################################\n#\n# Set static routes\n#\n##################################################\n - path: /etc/network/interfaces\n   permissions: '0644'\n   owner: root:root\n   content: |\n      auto lo\n      iface lo inet loopback\n\n      auto eth0\n      iface eth0 inet dhcp\n\n      auto eth1\n      iface eth1 inet dhcp\n\n      up route add -net 172.16.0.0/12 gw 192.168.3.1\n      up route add -net 10.0.0.0/8 gw 192.168.3.1\n\nruncmd:\n  - service networking restart\n"
  },
  "resources": [
    {
      "name": "[parameters('MOJumpBoxLinux_Availability_SetName')]",
      "type": "Microsoft.Compute/availabilitySets",
      "sku": {
        "name": "Aligned"
      },
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-04-30-preview",
      "dependsOn": [],
      "tags": {
        "displayName": "MOJumpBoxLinux_Availability_Set"
      },
      "properties": {
        "platformUpdateDomainCount": 5,
        "platformFaultDomainCount": 2
      }
    },
    {
      "name": "[parameters('VDSSJumpBoxLinux_Availability_SetName')]",
      "type": "Microsoft.Compute/availabilitySets",
      "sku": {
        "name": "Aligned"
      },
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-04-30-preview",
      "dependsOn": [],
      "tags": {
        "displayName": "VDSSJumpBoxLinux_Availability_Set"
      },
      "properties": {
        "platformUpdateDomainCount": 5,
        "platformFaultDomainCount": 2
      }
    },
    {
      "name": "[parameters('MOJumpBox_Availability_SetName')]",
      "type": "Microsoft.Compute/availabilitySets",
      "sku": {
        "name": "Aligned"
      },
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-04-30-preview",
      "dependsOn": [],
      "tags": {
        "displayName": "MOJumpBox_Availability_Set"
      },
      "properties": {
        "platformUpdateDomainCount": 5,
        "platformFaultDomainCount": 2
      }
    },
    {
      "name": "[variables('MOJumpBoxNicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]"
      ],
      "tags": {
        "displayName": "MOJumpBoxNic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAddress": "[parameters('MOJumpBoxIp')]",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[variables('MOJumpBoxSubnetRef')]"
              }
            }
          }
        ]
      }
    },
    {
      "name": "[parameters('MOJumpBoxName')]",
      "type": "Microsoft.Compute/virtualMachines",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-04-30-preview",
      "dependsOn": [
        "[resourceId('Microsoft.Storage/storageAccounts', variables('vdssstorageName'))]",
        "[resourceId('Microsoft.Network/networkInterfaces', variables('MOJumpBoxNicName'))]",
        "[concat('Microsoft.Compute/availabilitySets/', parameters('MOJumpBox_Availability_SetName'))]"
      ],
      "tags": {
        "displayName": "MOJumpBox"
      },
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', parameters('MOJumpBox_Availability_SetName'))]"
        },
        "hardwareProfile": {
          "vmSize": "[variables('MOJumpBoxVmSize')]"
        },
        "osProfile": {
          "computerName": "[parameters('MOJumpBoxName')]",
          "adminUsername": "[parameters('MOJumpBoxAdminUsername')]",
          "adminPassword": "[parameters('MOJumpBoxAdminPassword')]"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "[variables('MOJumpBoxImagePublisher')]",
            "offer": "[variables('MOJumpBoxImageOffer')]",
            "sku": "[parameters('MOJumpBoxWindowsOSVersion')]",
            "version": "latest"
          },
          "osDisk": {
            "createOption": "fromImage",
            "managedDisk": {
              "storageAccountType": "Premium_LRS"
            }
          },
          "dataDisks": []
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('MOJumpBoxNicName'))]"
            }
          ]
        }
      }
    },
    {
      "name": "[variables('MOJumpBoxLinuxNicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [
        "[resourceId('Microsoft.Network/virtualNetworks', parameters('vnetName'))]",
        "[resourceId('Microsoft.Network/publicIPAddresses', variables('linux-MOJumpBox-ipName'))]"
      ],
      "tags": {
        "displayName": "MOJumpBoxLinuxNic"
      },
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAddress": "[parameters('MOJumpBoxLinuxIp')]",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[variables('MOJumpBoxLinuxSubnetRef')]"
              },
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('linux-MOJumpBox-ipName'))]"
              }
            }
          }
        ]
      }
    },
    {
      "name": "[parameters('MOJumpBoxLinuxName')]",
      "type": "Microsoft.Compute/virtualMachines",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-04-30-preview",
      "dependsOn": [
        "[resourceId('Microsoft.Storage/storageAccounts', variables('vdssstorageName'))]",
        "[resourceId('Microsoft.Network/networkInterfaces', variables('MOJumpBoxLinuxNicName'))]",
        "[concat('Microsoft.Compute/availabilitySets/', parameters('MOJumpBoxLinux_Availability_SetName'))]"
      ],
      "tags": {
        "displayName": "MOJumpBoxLinux"
      },
      "properties": {
        "availabilitySet": {
          "id": "[resourceId('Microsoft.Compute/availabilitySets', parameters('MOJumpBoxLinux_Availability_SetName'))]"
        },
        "hardwareProfile": {
          "vmSize": "[variables('MOJumpBoxLinuxVmSize')]"
        },
        "osProfile": {
          "computerName": "[parameters('MOJumpBoxLinuxName')]",
          "adminUsername": "[parameters('MOJumpBoxAdminUsername')]",
          "adminPassword": "[parameters('MOJumpBoxAdminPassword')]",
          
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "[variables('MOJumpBoxLinuxImagePublisher')]",
            "offer": "[variables('MOJumpBoxLinuxImageOffer')]",
            "sku": "[parameters('MOJumpBoxLinuxUbuntuOSVersion')]",
            "version": "latest"
          },
          "osDisk": {
            "createOption": "fromImage",
            "managedDisk": {
              "storageAccountType": "Premium_LRS"
            }
          },
          "dataDisks": []
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('MOJumpBoxLinuxNicName'))]"
            }
          ]
        }
      }
    },
    {
      "name": "[variables('linux-MOJumpBox-ipName')]",
      "type": "Microsoft.Network/publicIPAddresses",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [],
      "tags": {
        "displayName": "linux-MOJumpBox-ip"
      },
      "properties": {
        "publicIPAllocationMethod": "Dynamic"
      }
    },
    {
      "apiVersion": "2015-06-15",
      "type": "Microsoft.Network/routeTables",
      "name": "[parameters('Gateway_RouteTableName')]",
      "location": "[resourceGroup().location]",
      "tags": {
        "displayName": "Route Table - Gateway"
      },
      "properties": {
        "routes": [
          {
            "name": "RouteToInternet",
            "properties": {
              "addressPrefix": "0.0.0.0/0",
              "nextHopType": "Internet"
            }
          },
          {
            "name": "RouteToManagement",
            "properties": {
              "addressPrefix": "[parameters('Management_SubnetPrefix')]",
              "nextHopType": "VirtualAppliance",
              "nextHopIpAddress": "[parameters('F5_Ext_Trusted_IP')]"
            }
          },
          {
            "name": "RouteToVDMS",
            "properties": {
              "addressPrefix": "[parameters('VDMS_SubnetPrefix')]",
              "nextHopType": "VirtualAppliance",
              "nextHopIpAddress": "[parameters('F5_Ext_Trusted_IP')]"
            }
          }
        ]
      }
